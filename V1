	if Active_Fly == false then
		Active_Fly = true
		Fly()
	else
		Active_Fly = false		
	end	
end)
local Fly_Speed = 1 ; local Max_Fly_Speed = 15
local Fly_Speed_SlideBar, Fly_Speed_Bar, Fly_Speed_TextBox, Fly_Speed_ShowValue, Fly_Speed_Color, Fly_Speed_ResetButton = Create_Slide_Textbox(ModePlayer_SelectFuntion, "‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡∏Å‡∏≤‡∏£‡∏ö‡∏¥‡∏ô", "‡πÉ‡∏™‡πà‡∏Ñ‡πà‡∏≤‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏•‡∏¢‡∏Ñ‡∏£‡∏±‡∏ö", Fly_Speed)
